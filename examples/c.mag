import wasi "std/wasi";

fn main() {
  let hello = "Hello world\n";
  hello[2] = hello[2] + 1 as u8;
  wasi.fd_write(1, hello as i32, 1, 0);

  let jauhar: *i32 = 36;
  *jauhar = 44;
  let jauhar: *i32 = 40;
  *jauhar = 10;
  let jauhar: []i32 = 36 as []i32;
  let i: i32 = 0;
  while i < 10 {
    jauhar[i] = i;
    i = i + 1;
  }
}
