import fmt "std/fmt";
import mem "std/mem";
import wasi "std/wasi";
import slice "std/slice";

fn main() {
  let a_slice = slice.make_slice[i64](10);
  let i = 0;
  while i < 10 {
    a_slice[i] = i;
    i = i + 1;
  }

  let i = 0;
  while i < 10 {
    fmt.print_i64(a_slice[i]);
    wasi.fd_write(1, "\n" as i32, 1, 0);
    i = i + 1;
  }
}

